<configuration name="cdr_csv.conf" description="CDR CSV Format">
  <settings>
    <!-- 'cdr-csv' will always be appended to log-base -->
    <param name="log-base" value="/opt/cdr"/>
    <param name="default-template" value="pgsql"/>
    <!-- This is like the info app but after the call is hung up -->
    <!--<param name="debug" value="true"/>-->
    <param name="rotate-on-hup" value="true"/>
    <param name="legs" value="ab"/>
  </settings>
  <templates>
    <template name="pgsql">'${direction}','${uuid}','${call_uuid}','${sip_call_id}','${bridge_uuid}','${originator}','${caller_id_name}','${caller_id_number}','${destination_number}','${stripped_called_number}',${customer_interconnection_id},'${customer_interconnection_name}','${rtp_last_audio_codec_string}','${ep_codec_string}','${sip_user_agent}','${sip_from_user}',${start_stamp},${answer_stamp},${end_stamp},'${created_time}','${progress_media_time}','${answered_time}',${duration},${billsec},${billmsec},'${last_bridge_proto_specific_hangup_cause}','${hangup_cause}','${hangup_cause_q850}',${system_release_cause}',${customer_id},${customer_rate_id},${supplier_rate_id},${route_id},${customer_zone_id},${interconnection_id_1},${interconnection_id_2},${interconnection_id_3},${interconnection_id_4},${interconnection_id_5},${interconnection_id_6},${supplier_id},${supplier_interconnection_id},'${customer_prefix}','${customer_rate}','${supplier_rate}','${customer_name}','${customer_rate_name}','${supplier_rate_name}','${customer_lcr_name}','${customer_zone_name}','${customer_prefix_name}','${supplier_prefix}','${supplier_prefix_name}',${customer_start_balance},${supplier_start_balance},${rtp_audio_in_jitter_min_variance},${rtp_audio_in_jitter_max_variance},${rtp_audio_in_mean_interval},${rtp_audio_in_quality_percentage},${rtp_audio_in_mos},${customer_rate_currency_id},${supplier_rate_currency_id},'${sip_received_ip}','${sip_destination_ip}',${rtp_audio_in_packet_count},${rtp_audio_in_media_packet_count},${rtp_audio_in_skip_packet_count},${rtp_audio_in_flush_packet_count},${rtp_audio_out_packet_count},${rtp_audio_out_media_packet_count},${rtp_audio_out_skip_packet_count},'f'</template>
   </templates>
</configuration>
